<!DOCTYPE html>
<html>
  <head>
    <title>ObjectDashboard</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="success">
      <% if flash[:success] %>
        <p><%= flash[:success] %></p>
      <% end %>
    </div>

    <div class="alert">
      <% if flash[:alert] %>
        <p><%= flash[:alert] %></p>
      <% end %>
    </div>

    <%= yield %>
  </body>
</html>
